<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>TrainingProject1</display-name>
  <welcome-file-list>
    <welcome-file>home.jsp</welcome-file>
  </welcome-file-list>
  <listener>
    <listener-class>
             aricent.project.model.businesslogic.HibernateListener 
        </listener-class>
  </listener>
  <servlet>
    <servlet-name>LoginHandler</servlet-name>
    <servlet-class>aricent.project.controllers.LoginHandler</servlet-class>
  </servlet>
  <error-page>
  <error-code>500</error-code>
  <location>/errorPages/errorPage500.jsp</location>
</error-page>
<error-page>
  <error-code>404</error-code>
  <location>/errorPages/errorPage404.jsp</location>
</error-page>
</web-app>